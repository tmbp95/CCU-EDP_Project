{
  "name": "customers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "customer_id": {
      "type": "number",
      "id": true,
      "required": true
    },
    "login": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "customer_name": {
      "type": "string"
    },
    "customer_address": {
      "type": "string"
    },
    "creditcard": {
      "type": "string"
    },
    "lastlogin": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "transactions": {
      "type": "hasMany",
      "model": "transactions",
      "foreignKey": "client_id"
    },
    "creditCards": {
      "type": "hasMany",
      "model": "creditCards",
      "foreignKey": "holder"
    },
    "energy": {
      "type": "hasMany",
      "model": "energy",
      "foreignKey": "holder"
    }
  },
  "acls": [],
  "methods": {}
}
